<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Students List</title>
</head>
<body>
  <h1>Students List</h1>
  <ul>
    <% students.forEach(student => { %>
      <li><%= student.fullName %> - <%= student.code %> - <%= student.fieldOfStudies %></li>
    <% }); %>
  </ul>
</body>
</html>
